{
    "WordPress Block Editor InspectorControls": {
        "prefix": "wp.blockeditor.inspector",
        "body": [
          "import { registerBlockStyle } from '@wordpress/block-editor';",
          "",
          "<InspectorControls>        ",
          "    Your content or components",
          "</InspectorControls>"
        ],
        "description": "WordPress Block Editor InspectorControls"
    }, 

    "WordPress Block Editor Innerblocks": {
        "prefix": "wp.blockeditor.innerblocks",
        "body": [
          "import { InnerBlocks } from '@wordpress/block-editor';",
          "",
          "<InnerBlocks />"
        ],
        "description": "WordPress Block Editor Innerblocks"
    }, 

    "WordPress Block Editor AlignmentToolbar": {
        "prefix": "wp.blockeditor.aligntoolbar",
        "body": [
          "import { AlignmentToolbar } from '@wordpress/block-editor';",
          "import { BlockControls } from '@wordpress/block-editor';",
          "",
          "<BlockControls>",
          "    <AlignmentToolbar",
          "        value={ alignment }",
          "        onChange={ onChangeAlignment }",
          "    />",
          "</BlockControls>"
        ],
        "description": "WordPress Block Editor AlignmentToolbar"
    }, 

    "WordPress Block Editor BlockControls": {
        "prefix": "wp.blockeditor.blockcontrol",
        "body": [
          "import { BlockControls } from '@wordpress/block-editor';",
          "",
          "<BlockControls>",
          "    // Your content or component ",
          "</BlockControls>"
        ],
        "description": "WordPress Block Editor BlockControls"
    }, 

    "WordPress Block Editor RichText": {
        "prefix": "wp.blockeditor.richtext",
        "body": [
          "import { RichText } from '@wordpress/block-editor';",
          "",
          "<RichText",
          "    tagName=\"h2\"",
          "    className={ className }",
          "    value={ attributes.content }",
          "   onChange={ ( content ) => setAttributes( { content } ) }",
          "/>"
        ],
        "description": "WordPress Block Editor RichText"
      }, 

      "WordPress Block Editor PanelColorSettings": {
        "prefix": "wp.blockeditor.panelcolor",
        "body": [
          "import { PanelColorSettings } from '@wordpress/block-editor';",
          "",
          " <PanelColorSettings ",
          "        title=\"Icon Farbe\"",
          "        initialOpen={ false }",
          "        colorSettings={ }",
          "/>"
        ],
        "description": "WordPress Block Editor PanelColorSettings"
      }, 

      "WordPress Block Editor MediaPlaceholder": {
        "prefix": "wp.blockeditor.mediaplaceholder",
        "body": [
          "import { MediaPlaceholder } from '@wordpress/block-editor';",
          "",
          "<MediaPlaceholder onSelect = { }",
          "        allowedTypes = { [ 'image' ] }",
          "        multiple = { false }",
          "        labels = { { title: 'The Image' } } >",
          "        \"extra content\"    ",
          "</MediaPlaceholder>;"
        ],
        "description": "WordPress Block Editor MediaPlaceholder"
      }
}